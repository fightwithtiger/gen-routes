{
  "name": "gen-routes",
  "version": "0.0.4",
  "packageManager": "pnpm@6.32.3",
  "description": "Dynamically generate routes based on the pages that can be viewed by the role. Used it in vue„ÄÅvue-router program.",
  "keywords": [
    "gen-routes",
    "dynamic routes",
    "vue-router"
  ],
  "homepage": "https://github.com/fightwithtiger",
  "bugs": {
    "url": "https://github.com/fightwithtiger/gen-routes/issues"
  },
  "license": "MIT",
  "author": "deng_tuo <2284925818@qq.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/fightwithtiger/gen-routes.git"
  },
  "funding": "https://github.com/fightwithtiger",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "play": "vite playground",
    "build": "rimraf dist && unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "pnpm build",
    "release": "bumpp --commit --push --tag && pnpm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@babel/types": "^7.17.0",
    "@types/node": "^17.0.23",
    "bumpp": "^7.1.1",
    "eslint": "^8.11.0",
    "esno": "^0.14.1",
    "pnpm": "^6.32.3",
    "rimraf": "^3.0.2",
    "typescript": "^4.6.2",
    "unbuild": "^0.7.0",
    "vite": "^2.8.6",
    "vitest": "^0.7.10"
  },
  "dependencies": {
    "vue-router": "^4.0.16"
  }
}
